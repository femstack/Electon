                                    <div class="review-comment">
                                        <div class="cmt-tit-count">
                                            <h6 class="comment-title" data-animate="animate__fadeInUp">
                                                <span class="cmt-title">Comment</span>
                                                <span class="cmt-count">({{blog.comments.length}})</span>
                                            </h6>
                                        </div>

                                        {{#each blog.comments}}
                                        <div class="cmt-info-wrap">
                                            <div class="comment-info" data-animate="animate__fadeInUp">
                                                <div class="comment-avtar">
                                                    <div class="review-name">
                                                        <span class="avtar-cmt">
                                                            <span
                                                                class="cmt-auth">{{this.author.[0]}}{{this.author.[1]}}</span>
                                                        </span>
                                                    </div>
                                                    <div class="review-info">
                                                        <span class="cmt-authr">{{this.author}}</span>
                                                        <span class="time">{{this.date}}</span>
                                                    </div>
                                                </div>
                                                <div class="comment-content">
                                                    <div class="comment-desc">
                                                        <p>{{this.message}}</p>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        {{/each}}
                                    </div>

                                    <div class="blog-comment-form">
                                        <form method="post" action="/article-post/{{blog._id}}/comment"
                                            class="comment-form">
                                            <div class="comments-reply-area">
                                                <h6 class="comment-title" data-animate="animate__fadeInUp">Leave a
                                                    comment</h6>
                                                <div class="form-wrap">
                                                    <div class="form-filed" data-animate="animate__fadeInUp">
                                                        <label>Name<span class="required">*</span></label>
                                                        <input type="text" name="author" required placeholder="Name">
                                                    </div>
                                                    <div class="form-filed" data-animate="animate__fadeInUp">
                                                        <label>Email address<span class="required">*</span></label>
                                                        <input type="email" name="email" required
                                                            placeholder="Email address">
                                                    </div>
                                                    <div class="form-filed" data-animate="animate__fadeInUp">
                                                        <label>Message<span class="required">*</span></label>
                                                        <textarea name="message" rows="5" class="comment-notes" required
                                                            placeholder="Message"></textarea>
                                                    </div>
                                                </div>
                                                <div class="comment-form-submit" data-animate="animate__fadeInUp">
                                                    <button class="btn btn-style2" type="submit">Post comment</button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>

                                </div>
                                <!-- blog post comment end -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
