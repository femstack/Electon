                                            <ul class="sidebar-tag">
                                                {{#each allTags}}
                                                <li data-animate="animate__fadeInUp">
                                                    <a href="/article-post/{{_id}}">{{this}}</a>
                                                </li>
                                                {{/each}}
                                            </ul>
                                        </div>
                                    </div> --}}
                                    <!-- blog-sidebar tag end -->

                                </div>
                                <!-- blog sidebar end -->
                            </div>
                            <div class="blog-article-wrap blog-article">
                                <!-- blog single-post start -->
                                {{#each findBlog}}
                                <div class="article-blog-post">
                                    <!-- blog img start -->
                                    <div class="blog-post-opt blog-post-img">
                                        <div class="blog-image">
                                            <a href="/article-post/{{_id}}" class="banner-img">
                                                <img src="{{image}}" class="img-fluid" alt="article-01">
                                            </a>
                                            <ul>
                                                <!-- blog-date start -->
                                                <li class="date-time">
                                                    <span>{{formatDate date}}</span>
                                                </li>
                                                <!-- blog-date end -->
                                                <!-- blog-comment start -->
                                                <li class="blog-comment">
                                                   {{#if blog.comments}} 
                                                   <span class="comment-count"> {{blog.comments.length}} comments</span>
                                                   {{/if}}
                                                </li>
                                                <!-- blog-comment end -->
                                            </ul>
                                        </div>
                                    </div>
                                    <!-- blog img start -->
                                    <!-- blog title start -->
                                    <div class="blog-post-opt blog-post-title">
                                        <div class="blog-revert">
                                            <h6 data-animate="animate__fadeInUp" class="post-title">{{title}}</h6>
                                            <!-- blog-info start -->
                                            <div class="post-info" data-animate="animate__fadeInUp">
                                                <span>{{excerpt}}</span>
                                            </div>
                                            <!-- blog-info end -->
                                        </div>
                                    </div>
                                    <!-- blog title end -->
                                    <!-- blog content start -->
                                    <div class="blog-post-opt blog-post-content">
                                        <div class="blog-content">
                                            <div class="blog-wrap-desc">
                                                {{content}}</p>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- blog content end -->
                                    <!-- blog tag start -->
                                    <div class="blog-post-opt blog-post-teg" data-animate="animate__fadeInUp">
                                        <div class="post-info-tag">
                                            <ul class="post-tag">
                                                {{#each this.tags}}
                                                <li data-animate="animate__fadeInUp">
                                                    <a href="/article-post/{{_id}}">{{this}}</a>
                                                </li>
                                                {{/each}}
                                            </ul>
                                        </div>
                                    </div>
                                    <!-- blog tag end -->
                                    <!-- blog share start -->
                                    <div class="blog-post-opt blog-post-icon" data-animate="animate__fadeInUp">
